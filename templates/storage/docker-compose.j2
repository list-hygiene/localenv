  storage:
    image: minio/minio:latest
    command: server /data --console-address ":9001"
    ports:
      - "1110:9000"
      - "1111:9001"
    environment:
      MINIO_ROOT_USER: minio_user
      MINIO_ROOT_PASSWORD: minio_password
      DOMAINS: minio.localhost
    volumes:
      - volume_storage:/data